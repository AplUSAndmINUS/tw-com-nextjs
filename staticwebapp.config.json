{
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": [
      "/_next/*",
      "/images/*.{png,jpg,gif,svg,ico,webp}",
      "/*.{css,js,json,xml,txt}"
    ]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
